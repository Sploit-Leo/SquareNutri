
@{
    ViewBag.Title = "Home Page";
    Layout = "~/Views/Shared/_LayoutPage1.cshtml";
}




<!--==========================
 Hero
============================-->


<section id="hero" class="wow fadeIn">

    <div class="hero-container">
        <h1>Bem-vindo(a) a Square Nutri</h1>
        <h2><b>Desperte o atleta em você</b></h2>
        <a href="#get-started" class="btn-get-started scrollto">Explorar</a>
    </div>

</section><!-- #hero -->
<!--==========================
  Consultas
============================-->

<section id="get-started" class="padd-section text-center wow fadeInUp">

    <div class="container">
        <div class="section-title text-center">

            <h2> Consultas </h2>
            <p class="separator">Desfrute da praticidade de nosso sistema de consultas online <br><b>Marque sua consulta já!</b></p>
            <div class="form">
                <br />
                @if ((string)Session["nivelLogado"] == "1")
                {
                    <div class="text-center"><button type="submit" data-toggle="modal" data-target="#modalConsulta">Marcar Consulta</button></div>
                }
                else if ((string)Session["nivelLogado"] == "2")
                {
                    <div class="text-center"><button type="submit"> Apenas Para Clientes</button></div>
                }


                else
                {
                    <div class="text-center"><button type="submit" data-toggle="modal" data-target="#modalLoginForm">  Marcar Consulta</button></div>
                }
                <!--==========================
                  Modal de Consultas
                ============================-->

                <div class="modal animated fadeInUp" id="modalConsulta" tabindex="-1" role="dialog" aria-labelledby="consultaModalTitle" aria-hidden="true">
                    <div class="modal-dialog" role="document">
                        <div class="modal-content">
                            <div class="modal-header">
                                <h2 class="modal-title" id="consultaModalTitle">Nova Consulta</h2>
                                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>

                            </div>
                            <div class="modal-body">

                                @using (Html.BeginForm("Index", "Consulta", FormMethod.Post, new { @class = "form", id = "formConsulta", role = "form", autocomplete = "off" }))
                                {
                                    <div class="form-group align-to-center">
                                        <h3>
                                            <label for="selectNutri">
                                                Selecione seu nutricionista: <button type="button" class="btn-link info-button" style="background-color:transparent; border: 0;" data-toggle="popover-hover"
                                                                                     data-placement="top" data-animation="true" data-content="Todos os nutricionistas da Square Nutri estão listados com suas respectivas especialidades.<br>Caso sua especialidade não esteja listada, selecione a opção <i>Geral</i>.">
                                                    <img src="~/Content/img/svg/info.svg" alt="img" class="img-fluid infosvg">
                                                </button>
                                            </label>
                                        </h3>


                                        @Html.DropDownList("codNutricionista", new List<SelectListItem>
                                    {
                                        new SelectListItem{ Text = "Nutricionistas", Disabled = true , Selected = true},
                                        new SelectListItem{ Text = "-", Disabled = true},
                                        new SelectListItem{ Text = "Alessandra Brandão - Surf/Tênis", Value = "1" },
                                        new SelectListItem{ Text = "Bruno Pereira - Artes Marciais/Boxe", Value = "2" },
                                        new SelectListItem{ Text = "Caio Bürgmann - Geral", Value = "3" },
                                        new SelectListItem{ Text = "Daniela Souza - Vôlei/Handebol", Value = "4" },
                                        new SelectListItem{ Text = "Felipe Gomes - Futebol/Basquete", Value = "5" },
                                        new SelectListItem{ Text = "Heloísa Leite - Natação", Value = "6" },
                                        new SelectListItem{ Text = "Juliana Campos - Ciclismo", Value = "7" },
                                        new SelectListItem{ Text = "Mariana Azevedo - Atletismo", Value = "8" }
                                    }
                                        , new {@class="form-control", id="selectNutri", required="required"})


                                    </div>

                                    <div class="datahoraForm" style="display:none">
                                        <br><hr><br>
                                        <div class="form-group">

                                            <h3>
                                                <label for="uname1">
                                                    Insira a data e hora desejados: <button type="button" class="btn-link info-button" style="background-color:transparent; border: 0;" data-toggle="popover-hover"
                                                                                            data-placement="top" data-animation="true" data-content="O limite máximo para uma consulta é de um mês a frente. ">
                                                        <img src="~/Content/img/svg/info.svg" alt="img" class="img-fluid infosvg">
                                                    </button>
                                                </label>
                                            </h3><br>
                                            <h4>Data Selecionada:</h4>
                                            @Html.Editor("dataConsulta", null, new { htmlAttributes = new { name = "dataConsulta", id = "dataConsulta", @type = "date", min = "@ViewBag.Min", max = "@ViewBag.Max", required = "required" } })
                                            @Html.ValidationMessage("dataConsulta")

                                            <br><br>
                                        </div>
                                        <br><br>

                                        <div class="form-group dataForm">
                                            <h4>
                                                <label>
                                                    Horários Disponíveis: <button type="button" class="btn-link info-button" style="background-color:transparent; border: 0;" data-toggle="popover-hover"
                                                                                  data-placement="top" data-animation="true" data-content="Atendemos de <i>segunda à sexta</i>, das <i>09h até 18h</i>. <br> Com um <b>plano da Square Nutri</b>, a disponibilidade muda para <i>segunda à sábado</i>, das <i>07h até 20h.</i>">
                                                        <img src="~/Content/img/svg/info.svg" alt="img" class="img-fluid infosvg">
                                                    </button>
                                                </label>
                                            </h4>

                                            <div class="custom-control custom-radio custom-control-inline">

                                                @Html.RadioButton("horaConsulta", "09h", new { @class = "custom-control-input form-control-gray hoverGlobal", id = "horaConsulta09", required = "required" })
                                                @Html.Label("horaConsulta09", "09h00", new { @class = "custom-control-label hoverGlobal" })

                                            </div>

                                            <div class="custom-control custom-radio custom-control-inline">

                                                @Html.RadioButton("horaConsulta", "10h", new { @class = "custom-control-input form-control-gray hoverGlobal", id = "horaConsulta10", required = "required" })
                                                @Html.Label("horaConsulta10", "10h00", new { @class = "custom-control-label hoverGlobal" })

                                            </div>

                                            <div class="custom-control custom-radio custom-control-inline">

                                                @Html.RadioButton("horaConsulta", "11h", new { @class = "custom-control-input form-control-gray hoverGlobal", id = "horaConsulta11", required = "required" })
                                                @Html.Label("horaConsulta11", "11h00", new { @class = "custom-control-label hoverGlobal" })

                                            </div>

                                            <div class="custom-control custom-radio custom-control-inline">
                                                @Html.RadioButton("horaConsulta", "13h", new { @class = "custom-control-input form-control-gray hoverGlobal", id = "horaConsulta13", required = "required" })
                                                @Html.Label("horaConsulta13", "13h00", new { @class = "custom-control-label hoverGlobal" })
                                            </div>

                                            <div class="custom-control custom-radio custom-control-inline">
                                                @Html.RadioButton("horaConsulta", "14h", new { @class = "custom-control-input form-control-gray hoverGlobal", id = "horaConsulta14", required = "required" })
                                                @Html.Label("horaConsulta14", "14h00", new { @class = "custom-control-label hoverGlobal" })
                                            </div>

                                            <div class="custom-control custom-radio custom-control-inline">
                                                @Html.RadioButton("horaConsulta", "15h", new { @class = "custom-control-input form-control-gray hoverGlobal", id = "horaConsulta15", required = "required" })
                                                @Html.Label("horaConsulta15", "15h00", new { @class = "custom-control-label hoverGlobal" })
                                            </div>

                                            <div class="custom-control custom-radio custom-control-inline">
                                                @Html.RadioButton("horaConsulta", "16h", new { @class = "custom-control-input form-control-gray hoverGlobal", id = "horaConsulta16", required = "required" })
                                                @Html.Label("horaConsulta16", "16h00", new { @class = "custom-control-label hoverGlobal" })
                                            </div>

                                            <div class="custom-control custom-radio custom-control-inline">
                                                @Html.RadioButton("horaConsulta", "17h", new { @class = "custom-control-input form-control-gray hoverGlobal", id = "horaConsulta17", required = "required" })
                                                @Html.Label("horaConsulta17", "17h00", new { @class = "custom-control-label hoverGlobal" })
                                            </div>

                                            <div class="custom-control custom-radio custom-control-inline">
                                                @Html.RadioButton("horaConsulta", "18h", new { @class = "custom-control-input form-control-gray hoverGlobal", id = "horaConsulta18", required = "required" })
                                                @Html.Label("horaConsulta18", "18h00", new { @class = "custom-control-label hoverGlobal" })
                                            </div>

                                        </div>

                                        <br><hr><br>

                                        <div class="form-group pagForm">
                                            <h3>
                                                <label for="selectPagamento">
                                                    Como você pretende pagar? <button type="button" class="btn-link info-button" style="background-color:transparent; border: 0;" data-toggle="popover-hover"
                                                                                      data-placement="top" data-animation="true" data-content="Selecione a forma de pagamento da consulta.<br>Caso você possua uma consulta sem custo devido a <b>assinatura de plano</b>,<br>selecione a opção <i>Presencial (Dinheiro)</i> e fale com nossos recepcionistas.">
                                                        <img src="~/Content/img/svg/info.svg" alt="img" class="img-fluid infosvg">
                                                    </button>
                                                </label>
                                            </h3>



                                            <select class="form-control" id="selectPagamento" required>

                                                <option selected="true" disabled="disabled" value="">Pagamento</option>
                                                <option disabled="disabled">-</option>
                                                <option id="pagPresen" value="valueConsulta" onclick="cartaoFalse()">Presencial</option>
                                                <option id="pagVirt" value="formCartao" onclick="cartaoTrue()">Virtual (Cartão)</option>
                                            </select>

                                            <script>
                                                function cartaoFalse() {
                                                    $(".cartaoPag").attr('required', false);
                                                }
                                                function cartaoTrue() {

                                                    $(".cartaoPag").attr('required', true);
                                                }
                                            </script>

                                        </div>
                                    </div>

                                    <input type="submit" id="btnMarcarConsulta" style="display:none">

                                }

                                <div class="form cartao-form" role="form" autocomplete="off" id="formCartao" style="display:none">
                                    <hr>
                                    <div class="form-group">

                                        <label for="titulCart">Titular do Cartão</label>
                                        <input type="text" class="form-control form-control-lg form-control-border-transition cartaoPag" form="formConsulta" name="titulCart" id="titulCart" placeholder="MARCO L PEIXOTO">
                                        <div class="invalid-feedback">Por favor insira o nome do titular do cartão.</div>
                                    </div>

                                    <div class="form-group">
                                        <label for="numCart">Número do Cartão</label>
                                        <input type="text" class="form-control form-control-lg form-control-border-transition cartaoPag" form="formConsulta" name="numCart" id="cartao-seg" placeholder="5417 6723 6297 6468">
                                        <div class="invalid-feedback">Por favor insira o número do cartão.</div>
                                    </div>

                                    <div class="form-group">
                                        <label for="codCart">Código de Segurança</label>
                                        <input type="text" class="form-control form-control-lg form-control-border-transition cartaoPag" form="formConsulta" name="codCart" id="cod-seg" placeholder="482">
                                        <div class="invalid-feedback">Por favor insira o código de segurança do cartão.</div>
                                    </div>

                                    <div class="form-group">
                                        <label for="dataCart">Data de Validade</label>
                                        <input type="text" class="form-control form-control-lg form-control-border-transition cartaoPag" form="formConsulta" name="dataCart" id="txt-data" placeholder="13/12/2021">
                                        <div class="invalid-feedback">Por favor insira a data de validade do cartão.</div>
                                    </div>

                                    <br>
                                </div>

                                <span style="display:none" id="valueConsulta"> <hr> <h3><b><i>Valor da Consulta:</i></b> R$449,90</h3> </span>

                            </div>
                            <div class="modal-footer">
                                <button type="button" class="btn btn-secondary" data-dismiss="modal">Fechar</button>
                                <label for="btnMarcarConsulta" class="btn btn-success btn-lg" tabindex="0"><i class="fa fa-clipboard-check"></i>  Marcar Consulta</label>
                            </div>=
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </div>

</section>


